# Add the provider to isntall gems into puppetserver
mod 'puppetlabs-puppetserver_gem', '1.0.0'

# Installs the acme/ohmyzsh module from the Puppet Forge
mod 'acme/ohmyzsh', '0.1.2'

# Installs the puppetlabs/vcsrepo module from its GitHub repository
mod 'vcsrepo',
  :git => 'https://github.com/puppetlabs/puppetlabs-vcsrepo'

# Standard Library is used by a lot of modules.
mod 'puppetlabs-stdlib', '4.17.1'

# EPEL management
mod 'stahnma-epel', '1.2.2'

# Windows registry
mod 'puppetlabs-registry', '1.1.4'

# Manage Redis
mod 'arioch-redis', '3.1.0'
mod 'puppetlabs-apt','2.3.0'
mod 'herculesteam-augeasproviders_sysctl','2.1.0'
mod 'herculesteam-augeasproviders_core','2.1.0'
